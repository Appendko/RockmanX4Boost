background_display:
mov byte ptr [RMX4.EXE+154903],cl
mov byte ptr [RMX4.EXE+154957],cl
ret

//function player_display(switch) --0x01 or 0x00
//	memory.writebyte(0x1419AB,switch);
//RMX4.EXE+144E83
//	if switch==0x00 then memory.writebyte(0x141A34,switch); end
//RMX4.EXE+144F0C
//end

player_display:
mov ch,cl
not ch
and ch,0x01
cmp ch,0x00
jne player_display_1
mov byte ptr [RMX4.EXE+144F0C],ch
player_display_1:
mov byte ptr [RMX4.EXE+144E83],ch
ret
//Stages

//if stage_modifier==0x04 and (area_modifier~=0x01 or checkpoint~=0x03) then
//	--火龍關，火球物件產生加速
