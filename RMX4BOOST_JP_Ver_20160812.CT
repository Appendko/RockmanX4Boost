CREATETHREAD(mycode)
//13E0A4 Timer1_JP
//13E0C4 Timer1_EN
//0x9C Space to next object 
//0x38 Space to timer2
//0x3F Space to timer3
//ebx直接當作enemy的Timer1位址，其他用ebx+0xXX來解決

mycode: 
push eax
push ebx
push ecx
push edx

//Initialization
xor ebx,ebx
mov [enemy_count],ebx
//RMX4.EXE+13E0A4 0x53E0A4
mov ebx,RMX4.EXE+13E0A4

//call fill_hp
//call fill_subtank

//0x53A004 是我方命數
//0x544EDC DD是我方血量
cmp byte ptr [RMX4.EXE+144EDC],0x00
je Fill_SubTank
cmp byte ptr [RMX4.EXE+144EDC],0x80
je Fill_SubTank
jmp Start_Stages

Fill_SubTank:
call fill_subtank
jmp Start_Stages
//if current_HP==0x00 then 
//	fill_subtank();
//end

//0x539FDD 是Checkpoint
//0x539FCC是Stage，0x539FCD是Area 0/1
//--00 Sky Lagoon / 01 Jungle / 02 Snow Base / 03 Bio Lab / 04 Volcano
//--05 Marine Base / 06 Cyber Space / 07 Air Force / 08 Military Train / 09 (估計是Memory Hall)
//--0A Space Port / 0B Final Weapon 1 / 0C Final Weapon 2 / 0D 選關 / 0E TITLE
Start_Stages:
cmp byte ptr [RMX4.EXE+139FCC],0x04
je Volcano
cmp byte ptr [RMX4.EXE+139FCC],0x05
je Marine_Base
cmp byte ptr [RMX4.EXE+139FCC],0x06
je Cyber_Space
cmp byte ptr [RMX4.EXE+139FCC],0x0C
je Final_Weapon_2
cmp byte ptr [RMX4.EXE+139FCC],0x0D
je Stage_Select


//Enemies
start_enemy_boost:
cmp [enemy_count],0x00
jne end_bosses
//	enemyid=memory.readdword(addr-0x14); //0x53E090,dword
cmp dword ptr [ebx-0x14],0x004D6898
je Eregion
cmp dword ptr [ebx-0x14],0x004D1A38
je Web_Spider
cmp dword ptr [ebx-0x14],0x004CEC20
je Split_Mushroom 
cmp dword ptr [ebx-0x14],0x004CDD68
je Magma_Dragoon
cmp dword ptr [ebx-0x14],0x004CF168
je Storm_Owl
cmp dword ptr [ebx-0x14],0x004CFA10
je Frost_Walrus
cmp dword ptr [ebx-0x14],0x004D0128
je Jet_Stingray
cmp dword ptr [ebx-0x14],0x004CD228
je Iris
cmp dword ptr [ebx-0x14],0x004CC768
je Sigma_1
cmp dword ptr [ebx-0x14],0x004CB208
je Sigma_2
cmp dword ptr [ebx-0x14],0x004D1370
je DG_42L
cmp dword ptr [ebx-0x14],0x004D4FF0
je Eyezard
cmp dword ptr [ebx-0x14],0x004D2FC8
je Tentoroid
//Cyber_Peacock 004CE368
//Slash_Beast 004D0810

end_bosses:
//		timer(addr)
call timer_fun
cmp dword ptr [ebx-0x14],0x004CBC80
je Raiden
cmp dword ptr [ebx-0x14],0x004D44A8
je Mega_Nests
cmp dword ptr [ebx-0x14],0x004D55C8
je Mettaur_D2
cmp dword ptr [ebx-0x14],0x004D3140
je Batton_Bone_B81

next_enemy_boost:

inc [enemy_count]
add ebx,0x9C
cmp [enemy_count],0x0D
jna start_enemy_boost

finishing: 
pop edx
pop ecx
pop ebx
pop eax
push 1
call sleep 
cmp [mustend],1 
jne mycode 
ret

//function timer_1(addr)
//	timer1byte=memory.readbyte(addr);
//	if timer1byte &gt; 0x02 then memory.writebyte(addr,0x01) end
//end

timer_1_fun:
//jna for unsign, jle for sign
cmp byte ptr [ebx],0x02
jna timer_1_fun_ret
mov byte ptr [ebx],0x01 
timer_1_fun_ret:
ret

//function timer_2(addr)
//	timer2word=memory.readword(addr+0x38);
//	if (timer2word &gt; 0x000F) then memory.writeword(addr+0x38,0x01) end
//end

timer_2_fun:
//jna for unsign, jle for sign
cmp word ptr [ebx+0x38],0x000F
jna timer_2_fun_ret 
mov word ptr [ebx+0x38],0x0001
timer_2_fun_ret:
ret

//function timer_3(addr)
//	--Timer3[[E113C033 0080 E113C033 0000 E113C033 00FF E313C033 0012 3013C033 0001]]--
//	timer3=memory.readbyte(addr+0x3F);	
//	if (timer3 ~= 0x80) and (timer3 ~= 0x00) and (timer3 ~= 0xFF) and (timer3 &gt; 0x12) then
//		memory.writebyte(addr+0x3F,0x01);
//	end	
//end
//不管~=00: 條件和&gt;12重疊

timer_3_fun:
//jna for unsign, jle for sign
cmp byte ptr [ebx+0x3F],0x80
je timer_3_fun_ret
cmp byte ptr [ebx+0x3F],0xFF
je timer_3_fun_ret
cmp byte ptr [ebx+0x3F],0x12
jna timer_3_fun_ret
mov byte ptr [ebx+0x3F],0x01
timer_3_fun_ret:
ret

//function timer(addr)
//	timer_1(addr);
//	timer_2(addr);
//	timer_3(addr); 
//end

timer_fun:
call timer_1_fun
call timer_2_fun
call timer_3_fun
ret

//function fill_hp()--滿HP
//	memory.writebyte(0x141A04,memory.readbyte(0x1722BE)); 
//end
fill_hp:
//0x544EDC 是我方血量, HP上限需要測試0x53A006
xor eax,eax
mov al, byte ptr [RMX4.EXE+13A006]
mov byte ptr [RMX4.EXE+144EDC], al
mov byte ptr [RMX4.EXE+154A02], al
add byte ptr [RMX4.EXE+154A02], al
dec byte ptr [RMX4.EXE+154A02]
ret

fill_subtank:
xor eax,eax
mov al, byte ptr [RMX4.EXE+13A01B]
mov ah,al
and ah,0x10
shr ah,4
cmp ah,0x01
jne fill_subtank_1
mov byte ptr [RMX4.EXE+13A01C],0x20
fill_subtank_1:
mov ah,al
and ah,0x20
shr ah,5
cmp ah,0x01
jne fill_subtank_2
mov byte ptr [RMX4.EXE+13A01D],0x20
fill_subtank_2:
mov ah,al
and ah,0x40
shr ah,6
cmp ah,0x01
jne fill_subtank_3
mov byte ptr [RMX4.EXE+13A01E],0x20
fill_subtank_3:
ret

